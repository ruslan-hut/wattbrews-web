{
  "name": "wattbrews-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config:dev": "node scripts/set-env.js",
    "verify:env": "node scripts/verify-env.js",
    "show:secrets": "node scripts/show-github-secrets.js",
    "prestart": "npm run config:dev",
    "start": "ng serve",
    "prebuild": "node scripts/set-env.js --prod",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.0",
    "@angular/cdk": "^21.1.0",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/fire": "^21.0.0-rc.0",
    "@angular/forms": "^21.1.0",
    "@angular/material": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@angular/service-worker": "^21.1.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@types/leaflet": "^1.9.20",
    "echarts": "^6.0.0",
    "firebase": "^11.10.0",
    "leaflet": "^1.9.4",
    "ngx-echarts": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "overrides": {
    "karma": {
      "glob": "^10.4.5",
      "rimraf": "^5.0.10"
    }
  },
  "devDependencies": {
    "@angular-eslint/schematics": "^21.1.0",
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@types/jasmine": "~5.1.0",
    "eslint": "^9.35.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.8.0",
    "typescript": "~5.9.2"
  }
}
